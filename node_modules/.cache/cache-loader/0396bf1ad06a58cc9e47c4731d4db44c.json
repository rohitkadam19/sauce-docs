AAQ,qCAD8D,CAAvB,EAE5C,yBAF4C,CAAnD,CAEsC,qEAFtC,CAZF,CAFK,CAkBL,aAAG,cAAQ,UAAU,CAAC,GAAnB,EAAyB,OAAzB,CAAH,CAA8C,8EAA9C,CAA4H,cAAQ,UAAU,CAAC,GAAnB,EAAyB,WAAzB,CAA5H,CAA2K,qHAA3K,CAlBK,CAAP,CAoBD,CAED,CACAD,UAAU,CAACG,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'config-okta',\n\ttitle: 'Configuring Okta',\n\tsidebar_label: 'Configuring Okta'\n};\nexport const metadata = {\n  \"unversionedId\": \"basics/sso/config-okta\",\n  \"id\": \"basics/sso/config-okta\",\n  \"isDocsHomePage\": false,\n  \"title\": \"Configuring Okta\",\n  \"description\": \"1. Log in to Okta with administrator credentials.\",\n  \"source\": \"@site/docs/basics/sso/config-okta.md\",\n  \"slug\": \"/basics/sso/config-okta\",\n  \"permalink\": \"/basics/sso/config-okta\",\n  \"editUrl\": \"https://github.com/saucelabs/sauce-docs/edit/master/docs/basics/sso/config-okta.md\",\n  \"version\": \"current\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar_label\": \"Configuring Okta\",\n  \"sidebar\": \"someSidebar\",\n  \"previous\": {\n    \"title\": \"Configuring Active Directory Federation Services (AD FS)\",\n    \"permalink\": \"/basics/sso/config-adfs\"\n  },\n  \"next\": {\n    \"title\": \"Overview of Sauce Labs Secure Connection Options\",\n    \"permalink\": \"/secure-connections\"\n  }\n};\n  /* @jsxRuntime classic */\n/* @jsx mdx */\n\nexport const toc = [];\n\nconst layoutProps = {\n  toc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <ol>\n      <li parentName=\"ol\">{`Log in to `}<strong parentName=\"li\">{`Okta`}</strong>{` with administrator credentials.`}</li>\n      <li parentName=\"ol\">{`On the `}<strong parentName=\"li\">{`Applications`}</strong>{` tab, search for and select the `}<strong parentName=\"li\">{`Sauce Labs`}</strong>{` application.`}</li>\n      <li parentName=\"ol\">{`Click `}<strong parentName=\"li\">{`Add`}</strong>{`.`}</li>\n      <li parentName=\"ol\">{`On the `}<strong parentName=\"li\">{`General Settings`}</strong>{` page of the setup wizard, enter an `}<strong parentName=\"li\">{`Application label`}</strong>{`, and next to `}<strong parentName=\"li\">{`Browser plugin auto-submit`}</strong>{`, select the `}<strong parentName=\"li\">{`Automatically log in when user lands on login page`}</strong>{` checkbox.`}</li>\n      <li parentName=\"ol\">{`Click `}<strong parentName=\"li\">{`Next`}</strong>{`.`}</li>\n      <li parentName=\"ol\">{`On the `}<strong parentName=\"li\">{`Sign-On Options`}</strong>{` page of the setup wizard, in the SAML 2.0 section, click the `}<strong parentName=\"li\">{`Identity Provider metadata`}</strong>{` link to download the metadata file. You will need to upload this file to Sauce Labs when you configure single sign-on.`}</li>\n      <li parentName=\"ol\">{`Under `}<strong parentName=\"li\">{`Credential Details`}</strong>{`, next to `}<strong parentName=\"li\">{`Application username format`}</strong>{`, select `}<strong parentName=\"li\">{`Email`}</strong>{`.`}</li>\n      <li parentName=\"ol\">{`Click `}<strong parentName=\"li\">{`Done`}</strong>{`.`}</li>\n      <li parentName=\"ol\">{`On the application details page, in the `}<strong parentName=\"li\">{`Assign`}</strong>{` dropdown, click `}<strong parentName=\"li\">{`Assign to People`}</strong>{`.`}</li>\n      <li parentName=\"ol\">{`In the `}<strong parentName=\"li\">{`Assign Sauce Labs to People`}</strong>{` window, next to the users or groups you want to give SSO access to, click `}<strong parentName=\"li\">{`Assign`}</strong>{`.`}</li>\n      <li parentName=\"ol\">{`Click `}<strong parentName=\"li\">{`Done`}</strong>{`.`}</li>\n      <li parentName=\"ol\">{`Follow the instructions in `}<a parentName=\"li\" {...{\n          \"href\": \"/basics/acct-team-mgmt/org-settings\"\n        }}>{`Single Sign-On Settings`}</a>{` to complete the integration of Sauce Labs with your Okta instance.`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`NOTE:`}</strong>{` The Sauce Labs app on the Okta Application Network only supports access to `}<strong parentName=\"p\">{`us-west-1`}</strong>{`. If you need to configure access for another data center, please follow the instructions for general setup of SSO.`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}